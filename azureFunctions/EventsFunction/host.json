{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "never",
    "logLevel": {
      "default": "None",
      "Host.Results": "Warning",
      "Function": "Warning",
      "Host.Aggregator": "Warning"
    },
    "applicationInsights": {
      "enableDependencyTracking": true,
      "samplingSettings": {
        "isEnabled": false,
        "excludedTypes": "Exception"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}